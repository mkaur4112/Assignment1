// Given an array of integers, sort the integer values.

public class Sortarray 
{
public static void main(String[] args)
{
int[] a= {8,2,3,1,6,4};
for(int j=0;j<a.length-1;j++)
{
for(int i=0;i<a.length-2;i++)
{
	   if(a[i]>a[i+1])
{
		   int temp=a[i];
			a[i]=a[i+1];
			a[i+1]=temp;
}
}
}
for(int j=0;j<a.length-1;j++)	
{
System.out.println(a[j]);
}
}
}
